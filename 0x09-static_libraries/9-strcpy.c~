#include "holberton.h"

/**
*_strcpy -
*@dest: string destination
*@src: string for copy  
*@n: size 
*Return: dest
*/

char *_strcpy(char *dest, char *src, size_t n)
{

size_t i;

      for (i = 0; i < n && src[i] != '\0'; i++)
	dest[i] = src[i];
      for ( ; i < n; i++)
	dest[i] = '\0';
      {
      return (dest);
      }
}
